<div class=''>
  <div class="xs-padded-bottom">
    <h1>Catégories de l'équipment</h1>
    <button class='btn btn-default btn-new'>Ajouter</button>
  </div>
  <table class="table table-striped table-bordered">
    <tr>
      <th>#</th>
      <th>Label</th>
    </tr>
    <% @categories.each do |category| %>
      <tr>
        <td><%= category.id %></td>
        <td><%= link_to category.label, category %></td>
      </tr>
    <% end %>
  </table>
</div>
